{
    "name": "emme-bi/sharing-linkedin",
    "description": "This package allows you to easily share content on Likedin, using the v2 API.",
    "keywords": ["linkedin", "linkedin share", "linkedin API v2"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": ">=6.3"
    },
    "authors": [
        {
            "name": "Mauro Bonaldo",
            "email": "mauro.bonaldo@emme-bi.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EmmeBi\\SharingLinkedin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmmeBi\\SharingLinkedin\\SharingLinkedinServiceProvider"
            ],
            "aliases": {
                "SharingLinkedin": "EmmeBi\\SharingLinkedin\\Facades\\SharingLinkedin"
            }
        }
    }
}
